# Django
SECRET_KEY=your-secret-key-here
DEBUG=False
ALLOWED_HOSTS=*.railway.app,localhost,127.0.0.1

# Database
DATABASE_URL=postgresql://user:password@host:port/database_name

# Railway automaticamente fornece DATABASE_URL para PostgreSQL
# Não é necessário configurar manualmente se usar o banco do Railway